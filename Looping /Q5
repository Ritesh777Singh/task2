// 5. WAP to print factorial of given number

#include <stdio.h>
int main(){
    int num;
    printf("Enter The Number :");
    scanf("%d",&num);
    int factorial = 1;
    if(num < 0){
        printf("factorial of a negative number doesn't exist");
    }
    else{
    for(int i = num; i>1 ; i--){
        factorial *= i;
    }
    printf("Factorial of %d = %d",num,factorial);
        
    }
}
