// 1. Write a C program to accept two integers and check whether they are equal or not

#include <stdio.h>
 int main()
 {
     int num1,num2;
     printf("enter the number1 : ");
     scanf("%d",&num1);
     printf("enter the number2 : ");
     scanf("%d",&num2);
     if(num1==num2){
         printf("integer is equal ");
         
     }else{
         printf("integer is not equal");
         
     }
    return 0;
}
