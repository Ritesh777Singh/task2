//find the area of a rectangular prism formula : A=2(wl+hl+hw)


#include <stdio.h>

int main() {
  int A,w,l,h;
  printf("enter the  number w : ");
  scanf("%d",&w);
  printf("enter the  number l : ");
  scanf("%d",&l);
  printf("enter the number h : ");
  scanf("%d",&h);
 A=2*(w*l+h*l+h*w);
  printf("totle number : %d",A);

    return 0;
}
